{
  "name": "LinkedClean",
  "description": "A plugin to keep Linkedin Clean!",
  "version": "0.7.4",
  "permissions": ["storage"],
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://*.linkedin.com/*"],
      "js": ["dist/content/content.js"]
    }
  ],
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "/images/linkedclean16.png",
      "32": "/images/linkedclean32.png",
      "48": "/images/linkedclean48.png",
      "128": "/images/linkedclean128.png"
    }
  },
  "icons": {
    "16": "images/linkedclean16.png",
    "32": "images/linkedclean32.png",
    "48": "images/linkedclean48.png",
    "128": "images/linkedclean128.png"
  }
}
